        <index referenceId="ABANDONED_CART_TABLE_STORE_ID" indexType="btree">
            <column name="store_id"/>
        </index>
        <index referenceId="ABANDONED_CART_TABLE_CUSTOMER_ID" indexType="btree">
            <column name="customer_id"/>
        </index>
        <index referenceId="ABANDONED_CART_TABLE_EMAIL" indexType="btree">
            <column name="email"/>
        </index>
    </table>
