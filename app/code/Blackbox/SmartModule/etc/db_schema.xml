        <constraint xsi:type="unique" referenceId="EMAIL_REVIEW_TABLE_REVIEW_ID">
            <column name="review_id"/>
        </constraint>
        <index referenceId="EMAIL_REVIEW_TABLE_CUSTOMER_ID" indexType="btree">
